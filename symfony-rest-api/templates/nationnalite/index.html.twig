{% extends 'base.html.twig' %}

{% block title %}Nationnalite index{% endblock %}

{% block body %}
    <h1>Nationnalite index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Libelle</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for nationnalite in nationnalites %}
            <tr>
                <td>{{ nationnalite.id }}</td>
                <td>{{ nationnalite.libelle }}</td>
                <td>
                    <a href="{{ path('nationnalite_show', {'id': nationnalite.id}) }}">show</a>
                    <a href="{{ path('nationnalite_edit', {'id': nationnalite.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('nationnalite_new') }}">Create new</a>
{% endblock %}
